//fibanachi
var mas_fib =  [0,1,1];
var i=3;
while(i<100){ 
mas_fib.push(mas_fib[i-1]+mas_fib[i-2]);
 i++;     
}
console.log(mas_fib);
// prime numbers
var mas_simp =  [1];

while(i<100){ 

for (var index = 2; index <=i; index++) {
	if(i%index==0)
	{
	    if(i==index)
	    mas_simp.push(i);
	    else
	   break;
	}
}
 i++;     
}
console.log(mas_simp);